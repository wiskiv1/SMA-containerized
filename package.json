{
  "name": "sma-containerized",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:worker\" \"next dev\"",
    "dev:nextjs": "next dev",
    "dev:worker": "nodemon --watch src/stock_market_anywhere --ext ts,tsx --exec tsx src/stock_market_anywhere/worker.ts",
    "build": "concurrently \"npm run build:worker\" \"next build\"",
    "build:nextjs": "next build",
    "build:worker": "tsc -p tsconfig.worker.json && tsc-alias -p tsconfig.worker.json",
    "start": "concurrently \"npm run start:worker\" \"next start\"",
    "start:nextjs": "next start",
    "start:worker": "node build/stock_market_anywhere/worker.js",
    "lint": "eslint"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "next": "16.1.1",
    "next-auth": "^4.24.13",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "nodemon": "^3.1.11",
    "tailwindcss": "^4",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
